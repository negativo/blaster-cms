<%  shared = JSON.parse(viewData); 
	if(shared.posts) posts = shared.posts;
	if(shared.page) page = shared.page;
	if(shared.navigation) navigation = shared.navigation;
	if(shared.links) links = shared.links;
%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title> <%= shared.title  %> </title>
	<link rel="stylesheet" href="/css/main.min.css">
	<link rel="stylesheet" href="/components/foundation/css/normalize.min.css">
	<link rel="stylesheet" href="/components/foundation/css/foundation.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
</head>
<body>
	<%= JSON.stringify(shared)  %>
	<!-- admin bar -->
	<% if(shared.isLoggedIn) { %>
		<%- include('../partials/admin-bar'); %>
	<% }; %>
	<!-- colored stripes useless -->
	<div class="ornament">
		<div class="object-bg"></div>
		<div class="function-bg"></div>
		<div class="parameter-bg"></div>
		<div class="statement-bg"></div>
	</div>
	<!-- MAIN WRAPPER -->
	<div class="site-wrapper small-10 small-offset-1 medium-8 medium-offset-2 large-6 large-offset-3">
		<!-- HEADER -->
		<header class="small-12 home">
			<h1><a href="/"><%= shared.site %></h1></a><span class="subtitle"><%= shared.subtitle %></span>
			<!-- NAVIGATION -->
			<%- include("../partials/nav") %>
		</header>

