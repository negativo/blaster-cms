<%- include("./partials/header") %>

<div class="container-fluid">
    <%= JSON.stringify(dashboard.users) %>
<!-- non Ã¨ un array per ora -->
    <!-- Table -->
    <table class="table table-striped">
    <tr>
        <th>#</th>
        <th>User</th>
        <th>Role</th>
    </tr>
    <% for (var i = 0; i < dashboard.users.length; i++) { %>
        <tr onclick="$.fn.getData(<%= JSON.stringify(dashboard.users[i]); %>)">
        	<td><%= i + 1 %></td>
            <td><%= dashboard.users[i].username %></td>
            <td><%= dashboard.users[i].admin %></td>
        </tr>
    <% }; %>
  </table>			


</div>
<!-- /.container-fluid -->

