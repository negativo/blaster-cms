<%- include("./partials/header") %>

<div class="container-fluid">
    <%= JSON.stringify(dashboard.users) %>
<!-- non Ã¨ un array per ora -->
    <!-- Table -->
    <table class="table table-striped">
    <tr>
        <th></th>
        <th>User</th>
        <th>Role</th>
        <th>Email</th>
    </tr>
    <% for (var i = 0; i < dashboard.users.length; i++) { %>
        <tr>
        	<td><a href="users/<%= dashboard.users[i]._id %>"><i class="fa fa-fw fa-cog fa-lg"></i></a></td>
            <td><%= dashboard.users[i].username %></td>
            <td><%= dashboard.users[i].role %></td>
            <td><%= dashboard.users[i].email %></td>
        </tr>
    <% }; %>
  </table>			


</div>
<!-- /.container-fluid -->

<%- include("./partials/footer") %>

